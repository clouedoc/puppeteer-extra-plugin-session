  "license": "MIT",
  "author": "Camille Lou√©doc <camille.eyries@gmail.com>",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "yarn tsc",
    "detect-circular-dependencies": "madge --circular --extensions ts ./src",
    "dev": "nodemon --exec 'yarn build && yarn lint && yarn detect-circular-dependencies && yarn sort'",
    "lint": "eslint ./src --fix --max-warnings 0",
    "publish": "yarn build && yarn publish",
    "sort": "npx sort-package-json",
    "test": "yarn jest --watchAll",
    "todo": "leasot -x --reporter markdown 'src/**/*.ts' --tags STEALTH > TODO.md"
  },
  "dependencies": {
    "puppeteer-extra-plugin": "^3.1.9",
    "tslib": "^2.1.0",
    "zod": "^3.0.0-alpha.33"
  },
  "devDependencies": {
    "@rushstack/eslint-config": "^3.0.1",
    "@types/jest": "^26",
    "@types/node": "^14",
    "eslint": "^7",
    "eslint-config-prettier": "^8",
    "eslint-config-prettier-standard": "^4",
    "eslint-config-standard": "^16",
    "eslint-plugin-import": "^2",
    "eslint-plugin-node": "^11",
    "eslint-plugin-prettier": "^3",
    "eslint-plugin-promise": "^4",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^26.6.3",
    "leasot": "^12.0.0",
    "madge": "^5.0.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.2.1",
    "prettier-config-standard": "^4",
    "puppeteer": "^5.5.0",
    "puppeteer-core": "^8.0.0",
    "puppeteer-extra": "^3.1.18",
    "puppeteer-extra-plugin-stealth": "^2.7.8",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
