    "puppeteer",
    "puppeteer-extra",
    "puppeteer-extra-plugin",
    "session",
    "puppeteer-extra-plugin-stealth",
    "cookies",
    "indexeddb",
    "profile",
    "sessionStorage",
    "localStorage"
  ],
